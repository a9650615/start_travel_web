     <script>
         /* cahgne page indicator */
         let show = 5;
         let startPage = @Search_result_currentpage;
         //console.log(page)
         let totalLength = $('.page').length
         function update_page() {
             let start = (startPage - Math.floor(show/2)) < 1? 1: (startPage - Math.floor(show/2))
             let end = (start+show-1);
             if (end >= totalLength) {
                 start -= (end - totalLength)
                 end = totalLength
             }
             $('.page').hide().removeClass('active').eq(startPage-1).addClass('active')
             for(let i = start; i<=end; i++) {
                 $('.page').eq(i-1).show()
             }

             if (startPage >= totalLength) {
                 $('.go-next').addClass('disable')
             }

             if (startPage <= 1) {
                 $('.go-back').addClass('disable')
             }
         }

         function left_button_click() {
             $('.go-back').removeClass('dsiable')
             if (startPage > 1)
                 //startPage--
                 to_page(--startPage)
             //    startPage --
             //update_page()
         }

         function right_button_click() {
             $('.go-next').removeClass('dsiable')
             if (startPage < totalLength)
                 //startPage++
                 to_page(++startPage)
             //    startPage ++
             //update_page()
         }

         update_page()
     </script>


