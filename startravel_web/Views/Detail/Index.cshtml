   <!--城市轉換-->
        @helper city_transfer(string str)
        {
            string city_to_ch = "";
            if (str.Equals("TPE"))
            {
                  city_to_ch="台北";


            }
            else if (str.Equals("BKK"))
            {
                city_to_ch = "曼谷";
                
            }else if (str.Equals("DMK"))
            {
                city_to_ch = "曼谷";

            }
            else if (str.Equals("TXG"))
            {
                city_to_ch = "台中";
            }
            else if (str.Equals("KHH"))
            {
                city_to_ch = "高雄小港";

            }
            else if (str.Equals("JP"))
            {
                city_to_ch = "日本";
            }
            
            
            
            
            
            
             @city_to_ch
       

          
                        
        }
    <div class="content content-white">
        <div class="star-container">
