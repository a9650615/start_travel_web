                DateTime igrptraffic_e = DateTime.ParseExact(Html.DisplayFor(model => model.grpproductdetail_result.Data.TrafficList[1].DATE_AT).ToString(), "yyyyMMdd", System.Globalization.CultureInfo.InvariantCulture);
                string min_tourer = Html.DisplayFor(model => model.grpproductdetail_result.Data.MIN_TOURER).ToString();
